format short
format compact

fclose(instrfind);
 s = serial('COM8');
 set(s, 'Baudrate',115200);
 fopen(s);
pause(2);
fprintf(s, '%c10','a');
out  = NaN;
out2 = NaN;
for i = 1:256
    value = fscanf(s, '%s',8);
    if length(value)>0
        out([1;i]) = fscanf(s, '%s',8);
    else disp('couldn\'t read')
    end
end
%% 
fclose(instrfind);
delete(s);
clear s